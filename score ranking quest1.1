#include <iostream>
using namespace std;
int main(int argc, const char * argv[])
{
    int score ;
    cout << "please enter your score :" << endl;
    cin >> score ;
    if (score >= 90)
        cout << "5" << endl;
    else if (score >= 80)
        cout << "4" << endl;
    else if (score >= 70)
        cout << "3" << endl;
    else if (score >= 60)
        cout << "2" << endl;
    else
        cout << "1" << endl;
    return 0;
}
