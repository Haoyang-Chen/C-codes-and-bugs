#include <iostream>
using namespace std;
int main(int argc, const char * argv[])
{
    int score ;
    int pp ;
    cout << "please enter your score" << endl;
    cin >> score ;
    pp = score/10;
    switch(pp)
    {
        case 9: cout << "5" << endl; break;
        case 8: cout << "4" << endl; break;
        case 7: cout << "3" << endl; break;
        case 6: cout << "2" << endl; break;
        default: cout << "1" << endl;
    }
    return 0;
}
